{
  "name": "jdhomepage",
  "version": "1.0.0",
  "description": "My homepage built with React.",
  "private": true,
  "jest": {
    "setupFiles": [
      "./src/tests/setup/jest.init.js"
    ]
  },
  "scripts": {
    "start": "node dist/server/jdserver.js",
    "startdev": "babel-node src/server/home.js",
    "starthot": "nodemon --exec babel-node src/server/home.js",
    "starthome": "webpack-dev-server --mode development --config ./config/webpack/webpack.config.home.js --port 5261 --open --hot",
    "startimposter": "webpack-dev-server --mode development --config ./config/webpack/webpack.config.imposter.js --port 5261 --open --hot",
    "startpw": "webpack-dev-server --mode development --config ./config/webpack/webpack.config.pw.js --port 5261 --open --hot",
    "startsurvive": "webpack-dev-server --mode development --config ./config/webpack/webpack.config.survive.js --port 5261 --open --hot",
    "starttut": "webpack-dev-server --mode development --config ./config/webpack/webpack.config.devtut.js --port 5261 --open --hot",
    "build": "npm-run-all -s build:apps build:css build:server copy:assets",
    "build:server": "webpack --mode=production --config=config/webpack/buildserver.config.js",
    "build:apps": "webpack --mode=production --config=config/webpack/buildapps.config.js",
    "build:css": "gulp --gulpfile config/gulpBuildCss.js",
    "copy:assets": "gulp --gulpfile config/gulpCopyAssets.js",
    "lint": "eslint --ignore-path ./config/.eslintignore src",
    "test": "jest"
  },
  "author": "John DeMey",
  "license": "ISC",
  "dependencies": {
    "@reach/router": "^1.3.4",
    "argv": "0.0.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.1.1",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "npm": "^6.14.6",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-particles-js": "^2.7.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "winston": "^3.2.1",
    "ws": "^7.4.5",
    "xss-clean": "^0.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtdemey/reacthomepage.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@material-ui/core": "^4.4.2",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-display-name": "^2.0.0",
    "bluebird": "^3.7.1",
    "clean-webpack-plugin": "^3.0.0",
    "colors": "^1.3.3",
    "compression": "^1.7.4",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.4.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-watch": "^6.0.1",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-plugin-remove": "^0.1.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^7.1.1",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "phaser": "^3.23.0",
    "postcss": "^7.0.18",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.7.2",
    "react-spring": "^8.0.27",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "tailwindcss": "^1.1.2",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  }
}
